<idea-plugin visibility="internal">
  <dependencies>
    <module name="intellij.editorconfig.backend"/>
    <module name="intellij.editorconfig.common"/>
    <module name="intellij.spellchecker"/>
  </dependencies>

  <extensions defaultExtensionNs="com.intellij">
    <spellchecker.support
      language="EditorConfig"
      implementationClass="com.intellij.editorconfig.backend.spellchecking.EditorConfigSpellcheckerStrategy"/>
  </extensions>
</idea-plugin>
