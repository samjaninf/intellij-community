<idea-plugin>
  <dependencies>
    <!-- region Generated dependencies - run `Generate Product Layouts` to regenerate -->
    <plugin id="com.intellij.completion.ml.ranking"/>
    <plugin id="org.jetbrains.plugins.gradle"/>
    <module name="intellij.gradle.java.groovy"/>
    <module name="intellij.java.psi"/>
    <!-- endregion -->
  </dependencies>

  <extensions defaultExtensionNs="com.intellij">
    <completion.ml.contextFeatures language="Groovy" implementationClass="com.intellij.gradle.java.groovy.ml.GradleGroovyContextFeatureProvider"/>
    <completion.ml.elementFeatures language="Groovy" implementationClass="com.intellij.gradle.java.groovy.ml.GradleGroovyElementFeatureProvider"/>
    <completion.ml.ranking.features.policy language="Groovy" implementationClass="com.intellij.gradle.java.groovy.ml.GradleGroovyCompletionFeaturesPolicy"/>
  </extensions>
</idea-plugin>
